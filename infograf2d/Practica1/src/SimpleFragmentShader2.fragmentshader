#version 330 core

struct Material {
    sampler2D diffuse;
    sampler2D specular;
    float     shininess;
};   

in vec3 FragPos;  
in vec3 Normal;  
in vec2 TexCoords;
  
  
uniform Material material;

out vec4 color;

void main()
{	
   color = texture(material.diffuse, TexCoords);
}
